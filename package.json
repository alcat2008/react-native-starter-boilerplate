{
  "name": "StarterBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "dev": "JS_DIR=`pwd`; cd node_modules/react-native; npm run start -- --root $JS_DIR  --reset-cache",
    "release": "rm -rf $TMPDIR/react-*; rm ios/main.jsbundle; react-native bundle --platform ios --dev false --entry-file index.ios.js --bundle-output ios/main.jsbundle --assets-dest ios/",
    "lint": "eslint ./app",
    "validate": "npm ls",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "lodash": "^4.14.2",
    "mx-artifacts": "^0.2.9",
    "react": "15.2.1",
    "react-native": "^0.30.0",
    "react-native-button": "^1.6.0",
    "react-native-router-flux": "^3.32.0",
    "react-native-vector-icons": "^2.0.3",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "5.0.0",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-jsx-a11y": "1.4.2",
    "eslint-plugin-react": "5.1.1",
    "estraverse-fb": "1.3.1",
    "precommit-hook-eslint": "3.0.0",
    "remote-redux-devtools": "^0.4.0"
  },
  "author": "ge_yangyang@126.com",
  "pre-commit": [
    "lint"
  ]
}
